  "dependencies": {
    "@react-hook/resize-observer": "^1.2.0",
    "@u-wave/react-vimeo": "^0.9.4",
    "disqus-react": "^1.0.11",
    "react-yandex-maps": "^4.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@quarkly/sdk": "^0.5.2",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "main": "./index.js",
  "peerDependencies": {
    "@quarkly/atomize": "^1.0.8",
    "@quarkly/components": "^0.2.48",
    "@quarkly/widgets": "^0.2.48",
    "react": "^16.13.0",
    "styled-components": "^5.0.1"
  },
  "quarklyInfo": {
    "components": {
      "Slider": "Slider",
      "Test": "Test",
      "Audio": "Audio",
      "DisqusComment": "DisqusComment",
      "Disqus": "Disqus",
      "Vimeo": "Vimeo",
      "SvgShape": "SvgShape",
      "YandexMap": "YandexMap"
    }
  },
  "repository": "https://github.com/quarkly/community-kit",
  "scripts": {
    "build": "npm run clean && npm run build:cjs && cp package.json ./build && cp README.md ./build && npm run copy_readme",
    "build:cjs": "NODE_ENV=cjs babel src --out-dir build --ignore '**/*.test.js'",
    "clean": "rimraf build/*",
    "copy_readme": "cd ./src && find . -name '*.md' -exec cp --parents {} ../build \\;",
    "create:component": "quarkly-sdk component $1",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "sideEffects": false
}
